<script>window.map = L.mapbox.map('map','codeforamerica.map-stwhr1eg');</script>

		<div class="container-fluid">
			<p id="notice"><%= notice %></p>
			<div class="row-fluid">
				<div class="span6 offset1">
					<div class="infoBox" style="margin-bottom: 0;">
						<div class="upper" style="border-left: 8px solid #FDBB11;">
							<div class="row">
								<div class="span5 offset1">
									<div class="infoSet_above">
										address
									</div>
									<div class="address">
									  	<%= @subject.name %><br>
									</div>
									<% if @subject.type == "Property" %>
									<% end %>
								</div>
								<div class="span5 offset1">
									<div class="row">
										<div class="span2 infoLogo">
											<img src="/assets/<%= @subject.property_info_set.condition.gsub(" ", "") %>.png" />
										</div>
										<div class="span9">
											<div class="infoSet_above">
												inspector assesment
											</div>
											<div class="infoSet_below">
												<%= @subject.property_info_set.condition %>
											</div>
										</div>
									</div>
									<div class="row">
										<div class="span2 infoLogo">
											<img src="/assets/<%= @subject.property_info_set.recommendation %>.png" />
										</div>
										<div class="span9">
											<div class="infoSet_above">
												inspector recommendation
											</div>
											<div class="infoSet_below">
												<%= @subject.property_info_set.recommendation %>
											</div>
										</div>
									</div>
									<div class="row">
										<div class="span2 infoLogo">
											<img src="/assets/<%= @subject.property_info_set.outcome.gsub(/(\s|[\/])/, "") %>.png" />
