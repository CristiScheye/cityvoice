										<div class="span9">
											<div class="infoSet_above">
												status
											</div>
											<div class="infoSet_below">
												<%= @subject.property_info_set.outcome %>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div id="map"></div>
					<div class="infoBox">
						<div class="lower">
							<div class="row resources">
								<div class="span4 offset1 resources_name">
									<a href="http://www.nrc4neighborhoods.org/neighborhoods/14-monroe-park/">Monroe Park Neighborhood Association</a>
								</div>
								<div class="span6 resources_info">
									Salvation Army Building, 520 S. Carroll Street<br>
